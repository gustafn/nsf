nx::test case nsf-proc-rename-redefine {
  namespace eval ::ns1 {}
  
  nsf::proc ns1::foo {
    {-x:required}
  } { return 1-$x }

  ? {ns1::foo -x ok} 1-ok
  
  rename ns1::foo ns1::foo.orig

  nsf::proc ns1::foo {
    {-x:required}
  } { return 2-$x }

  ? {ns1::foo -x ok} 2-ok

  namespace delete ::ns1
}

